<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HvZUAB.StartPage">
    <ContentPage.Content>
        <Grid RowSpacing="18">
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="200"/>
            </Grid.RowDefinitions>

            <StackLayout Grid.Row="1">
                
            </StackLayout>
            
            <StackLayout Grid.Row="2">
                
                <Button WidthRequest="134" HeightRequest="50"
                        Text="Register" FontSize="15" HorizontalOptions="Center" VerticalOptions="Center" Clicked="RegisterNavClicked"/>


                <Button WidthRequest="134" HeightRequest="50"
                       Text="Log In" FontSize="15" HorizontalOptions="Center" VerticalOptions="Center" Clicked="LogInNavClicked"/>

            </StackLayout>

        </Grid>
            
        
                

            
    </ContentPage.Content>
</ContentPage>